{
  "expo": {
    "name": "Easy Rent Sale",
    "slug": "EasyRentSale",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#318080"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.EasyRentSale.EasyRentSale",
      "supportsTablet": true
    },
    "android": {
      "package": "com.EasyRentSale.EasyRentSale"
    },
    "packagerOpts": {
      "nonPersistent": true
    }
  }
}
